{
  "name": "poc-lambda-template-base",
  "version": "1.0.0",
  "description": "Lambda para POC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.0",
    "@aws-sdk/client-secrets-manager": "^3.53.0",
    "aws-sdk": "^2.1038.0",
    "axios": "^0.23.0",
    "class-transformer": "^0.5.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.67.0",
    "@types/aws-lambda": "^8.10.75",
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.0",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "aws-sdk-client-mock": "^0.6.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^27.0.6",
    "json-schema-to-ts": "^1.6.4",
    "prettier": "^2.2.1",
    "serverless": "^2.67.0",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-esbuild": "^1.22.0",
    "serverless-offline": "^8.3.1",
    "serverless-plugin-typescript": "^2.1.0",
    "serverless-plugin-warmup": "^6.0.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.2.4",
    "semantic-release": "^20.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^9.0.0",
    "conventional-changelog-eslint": "^3.0.9",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@swc/jest": "^0.2.24"
  },
  "scripts": {
    "compile": "tsc",
    "build": "swc --ignore \"**/*.spec.ts,hmr.ts\" --delete-dir-on-start --out-dir build/src/ src/ && cp package.json .npmrc pnpm-lock.yaml build/ && cd build && pnpm install  --node-linker hoisted --prod && rm .npmrc",
    "build:ts": "cp package.json build/package.json && cp .npmrc build/.npmrc && cd build && npm install --force --only=production && rm .npmrc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "test:cov": "jest --coverage --detectOpenHandles --passWithNoTests",
    "test:service": "jest \"./*/*.service.spec.ts\" --detectOpenHandles --passWithNoTests",
    "test:helper": "jest \"./*/*.helper.spec.ts\" --detectOpenHandles --passWithNoTests",
    "test:handler": "jest \"./src/handler.spec.ts\" --detectOpenHandles --passWithNoTests",
    "test:repository": "jest \"./*/*.repository.spec.ts\" --detectOpenHandles --passWithNoTests",
    "semantic-release": "semantic-release"
  },
  "pnpm": {
    "overrides": {
      "simple-git@<3.15.0": ">=3.15.0",
      "got@<11.8.5": ">=11.8.5",
      "jsonwebtoken@<9.0.0": ">=9.0.0"
    }
  }
}
